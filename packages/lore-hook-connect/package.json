{
  "name": "lore-hook-connect",
  "version": "0.12.0-rc.1",
  "license": "MIT",
  "main": "./lib/index.js",
  "description": "A lore hook that generates dialogs using Bootstrap",
  "keywords": [
    "lore",
    "hook",
    "dialogs",
    "bootstrap"
  ],
  "scripts": {
    "build": "../../node_modules/.bin/babel src --out-dir lib",
    "clean": "rimraf lib",
    "debug": "mocha debug --compilers js:babel-core/register --recursive",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register test/bootstrap.js test/**/*.spec.js --recursive"
  },
  "dependencies": {
    "invariant": "2.2.0",
    "lodash": "3.10.1",
    "lore-utils": "^0.12.0-rc.1"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "mocha": "2.3.4",
    "react": "^15.4.0",
    "rimraf": "2.5.2",
    "sinon": "^1.17.7"
  },
  "peerDependencies": {
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.1"
  }
}
