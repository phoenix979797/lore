{
  "name": "@lore/connect",
  "version": "0.13.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "description": "A lore hook that orchestrates behavior between actions and reducers",
  "keywords": [
    "lore",
    "hook",
    "redux",
    "actions",
    "reducers"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "BABEL_ENV=cjs ../../node_modules/.bin/babel --root-mode upward src --out-dir lib",
    "build:es": "BABEL_ENV=es ../../node_modules/.bin/babel --root-mode upward src --out-dir es",
    "clean": "rimraf lib && rimraf es",
    "debug": "mocha debug --compilers js:babel-core/register --recursive",
    "lint": "../../node_modules/.bin/eslint src",
    "prepublish": "npm run build",
    "test": "BABEL_ENV=test mocha --compilers js:babel-core/register test/bootstrap.js test/**/*.spec.js --recursive"
  },
  "dependencies": {
    "invariant": "2.2.0",
    "lodash": "^4.0.0",
    "@lore/utils": "^0.13.0",
    "prop-types": "^15.5.0"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "mocha": "2.3.4",
    "react": "^16.1.1",
    "rimraf": "2.5.2",
    "sinon": "^1.17.7"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  }
}
