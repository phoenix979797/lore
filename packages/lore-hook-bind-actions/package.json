{
  "name": "@lore/hook-bind-actions",
  "version": "0.13.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "description": "A lore hook binds action to the Redux store",
  "keywords": [
    "lore",
    "hook",
    "actions",
    "redux"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "BABEL_ENV=cjs ../../node_modules/.bin/babel src --out-dir lib",
    "build:es": "BABEL_ENV=es ../../node_modules/.bin/babel src --out-dir es",
    "clean": "rimraf lib && rimraf es",
    "debug": "mocha debug --compilers js:babel-core/register --recursive",
    "lint": "../../node_modules/.bin/eslint src",
    "prepublish": "npm run build",
    "test": "BABEL_ENV=test mocha --compilers js:babel-core/register test/bootstrap.js test/**/*.spec.js --recursive"
  },
  "dependencies": {
    "lodash": "^4.0.0"
  },
  "devDependencies": {
    "chai": "3.4.1",
    "@lore/utils": "^0.13.0",
    "mocha": "2.3.4",
    "redux": "^3.0.2",
    "rimraf": "2.5.2"
  },
  "peerDependencies": {
    "redux": "^3.0.2"
  }
}
