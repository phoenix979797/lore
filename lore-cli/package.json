{
  "name": "lore-cli",
  "version": "0.3.2",
  "author": "Storcery",
  "description": "Command Line Interface for Lore (A convention driven framework for React + Redux applications)",
  "license": "MIT",
  "main": "packages/lore-cli-core/lib/index.js",
  "bin": {
    "lore": "packages/lore-cli-core/bin/lore.js"
  },
  "files": [
    "packages/lore-cli-core/bin"
  ],
  "scripts": {
    "clean": "monorepo --baseDirectories packages --removeDirectories node_modules lib coverage .tmp",
    "build": "monorepo --baseDirectories packages --install",
    "build:production": "monorepo --baseDirectories packages --install --production",
    "lint": "echo 'tbd: implement me.'",
    "check": "npm run lint && npm run test",
    "prepublish": "npm run clean && npm run build && npm run check",
    "test": "monorepo --baseDirectories packages --test",
    "debug": "monorepo --baseDirectories packages --command debug",
    "install": "npm run build:production"
  },
  "dependencies": {
    "monorepo": "0.1.8"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storcery/lore.git"
  },
  "bugs": {
    "url": "https://github.com/storcery/lore/issues"
  },
  "homepage": "https://github.com/storcery/lore#readme"
}
