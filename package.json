  "scripts": {
    "clean": "npm prune && rimraf lib coverage .tmp",
    "check": "npm run lint && npm run test",
    "preversion": "npm run clean && npm run check",
    "lint": "echo 'tbd: implement me.'",
    "build": "babel src --out-dir lib",
    "test": "npm run install-models && npm run install-actions && npm run clean && npm run build && mocha --compilers js:babel-core/register test/bootstrap.js test/unit test/integration --recursive",
    "debug": "mocha debug test/bootstrap.js test/unit test/integration --recursive",
    "install-models": "cd packages/lore-models && npm install",
    "install-actions": "cd packages/lore-actions && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/storcery/lore.git"
  },
